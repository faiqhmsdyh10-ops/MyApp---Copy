# Supabase Configuration
# Dapatkan dari: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
SUPABASE_URL=your_supabase_url_here
REACT_APP_SUPABASE_PUBLISHABLE_DEFAULT_KEY=your_publishable_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Server Configuration
PORT=5000

# Midtrans Configuration
# Dapatkan dari: https://dashboard.midtrans.com
# Gunakan Sandbox untuk testing, Production untuk live
MIDTRANS_SERVER_KEY=your_midtrans_server_key_here
MIDTRANS_CLIENT_KEY=your_midtrans_client_key_here
MIDTRANS_IS_PRODUCTION=false
